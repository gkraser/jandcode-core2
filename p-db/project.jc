import jandcode.jc.*
import jandcode.jc.std.*

class P extends ProjectScript {

    static beforeLoad = {
        load("..")
    }

    void onInclude() {
        //
        project.name = "p-db"
        project.version = load("..").version
        //
        include(RootProject).with {
            groupId = "com.jandcode"
            moduleGroup = "jc-core/p-db"
            modules = [
                //
                'jandcode-db',
                'jandcode-db-derby',
                'jandcode-db-mysql',
                'jandcode-db-oracle',
                'jandcode-db-firebird',
                'jandcode-db-postgresql',
            ]
        }
    }

}