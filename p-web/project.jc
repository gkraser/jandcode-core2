import jandcode.jc.*
import jandcode.jc.std.*

class P extends ProjectScript {

    static beforeLoad = {
        load("..")
    }

    void onInclude() {
        //
        project.name = "p-web"
        project.version = load("..").version
        //
        include(RootProject).with {
            groupId = "com.jandcode"
            moduleGroup = "jc-core/p-web"
            modules = [
                    'jandcode-undertow',
                    'jandcode-web',
                    'jandcode-web-tst',
                    'jandcode-web-jc',
            ]
        }
    }

}