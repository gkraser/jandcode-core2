void onInclude() {
    project.name = "jandcode-commons-groovy"
    //! еще нет модуля jc !//
    def jm = include("jandcode.jc.std.JavaProject")
    //
    jm.autoJcDepends = false
    jm.packageRoot = "jandcode.commons.groovy"
    jm.depends.prod.add(
            "jandcode-commons",
            "groovy",
            "groovy-ant",
            "groovy-json",
            "groovy-xml",
            "groovy-templates",
    )
    jm.depends.dev.add()
}
